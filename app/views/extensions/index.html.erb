<p id="notice"><%= notice %></p>

<h1>Proyecto Extencion</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Date start</th>
      <th>Date end</th>
      <th>State</th>
      <th>Manager</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @extensions.each do |extension| %>
      <tr>
        <td><%= extension.name %></td>
        <td><%= extension.description %></td>
        <td><%= extension.date_start %></td>
        <td><%= extension.date_end %></td>
        <td><%= extension.state %></td>
        <td><%= extension.manager %></td>
        <td><%= link_to 'Show', extension %></td>
        <td><%= link_to 'Edit', edit_extension_path(extension) %></td>
        <td><%= link_to 'Destroy', extension, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Extension', new_extension_path %>
